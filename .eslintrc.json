{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-unused-vars": [
            "warn"
        ],
        "require-jsdoc": [
            "warn"
        ],
        "indent": [
            "warn",
            4
        ],
        "prefer-const": [
            "warn"
        ]
    }
}
